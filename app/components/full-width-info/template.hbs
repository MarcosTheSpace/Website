{{yield}}
<div class="gray-background">
	<hr  size="40" class="bar">
	<div class="arrow-down"></div>
</div>

<div class="full-width-info gray text-center">
	<hr width="5" size="100" class="bar">
	<div class="text-center" style="margin-left:35px; margin-top:10px;">
		<div class="important-holder">
			<span class="important-text">Web</span><span class="important-text blue circle-div"> & </span><span class="important-text">Code</span>
			<span class="sub-text-left">Design</span><span class="sub-text-right">Development</span>


		</div>


	</div>
	<br>
	<p class="large-paragraph">An innovative, curious, hardworking individual with a BS in Information Technology from Rochester Institute of Technology. Enjoys working with and learning about technology. Successful, balanced IT education at RIT focusing on web and mobile. Working knowledge of web-based platforms and scripting. Strong interest in web design.</p>
	<hr width="5" size="100" class="bar">

</div>

{{info-block}}


<div class="full-width-info dark-gray" >


	<h2 class="text-center">&#60; Project Highlights &#47;&#62;</h2>

	<hr class="color">
	{{#each websiteJSON as |website index|}}
		<div class="flex-grid-wrap">
			<div class="col">
				<p class="large-paragraph text-center center-content-resize">
					<span class="website-title">{{website.websiteTitle}}</span>
					{{website.websiteDescription}}
					<div class="margin-auto" >
						{{#if website.git}}
							<a href="{{website.git}}" class="inline-block github" target=" _blank"></a>
						{{/if}}
						{{#if website.url}}
							<a href="{{website.url}}" class="inline-block www" target="_blank"></a>
						{{/if}}
					</div>
					</p>
			</div>
			<div class="col" style="text-align: center;"> 
				<div class="website-images">


					{{#each website.websiteURL as |url indexURL|}}

						{{#if (eq indexURL 0) }}
							<input type="radio" id="website-{{indexURL}}" name="contact{{index}}" value="website" class="website-radio" checked>
						{{else}}
							<input type="radio" id="website-{{indexURL}}" name="contact{{index}}" value="website" class="website-radio">
						{{/if}}
									    
				    {{/each}}

				 	{{#each website.websiteURL as |url indexURL|}}
   					    <div 
					    	for="website-{{indexURL}}" 
					    	class="website-image" 
					    	id="website-image-{{indexURL}}" 
					    	style="background: url({{url}}) no-repeat center;background-size: 560px 350px"
					    ></div>
					{{/each}}
		
				</div>
			</div>
		</div>
		<br>
		<hr class="color">
		<br>
	{{/each}}
	
	<style>
    {{#each websiteJSON as |website index|}}

		{{#each website.websiteURL as |url indexURL|}}
			#website-image-{{indexURL}}  {
				display:none;

			}
			#website-{{indexURL}}:checked ~ #website-image-{{indexURL}}  {
				display:block;
			}


		{{/each}}

    {{/each}}

	</style>


</div>



